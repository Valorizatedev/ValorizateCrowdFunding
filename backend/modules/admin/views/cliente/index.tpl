<form id="form1">	<div class="row" >		<div class="col-md-13">			<div class="col-md-4" >				<font class="brand" style="font-size:22px;" ><b>Clientes</b></font>											</div>			<div class="col-md-4" >						<div class="col-md-5 pull-right" >										{if $_acl->permiso('crear_cliente')}					<a class="btn btn-blue" href="{$_layoutParams.root}admin/cliente/nuevo"><i class="icon-plus"></i> Nuevo cliente</a>				{/if}			</div>				</div>			<div class="col-md-4 pull-right" >				<div class="input-group" >					<div class="input-group-addon">						<i class="entypo-search"></i>					</div>						<input type="text" class="form-control" name="buscar" id="buscar"  placeholder="Buscar">					</div>			</div>		</div>	</div>
	<hr>    <table class="table table-bordered table-hover" align="center" style="width: 100%;">
        <tr>            <th>Nombre(s)</th>            <th>Identificación</th>            <th>Teléfono</th>            <th colspan=""></th>        </tr>
		{foreach $clientes as $cliente}            <tr >                <td>                    {$cliente.cli_nombre}                </td>                <td>                    {$cliente.cli_num_documento}                </td>                <td>                    {$cliente.cli_telefono}                </td>                <td align="right">                    <a class="btn btn-primary" href="{$_layoutParams.root}admin/cliente/detalles/{$cliente.cli_key}"><i class="icon-eye-open"></i> Detalles</a>                {if $_acl->permiso('modificar_cliente')}                        <a class="btn btn-primary" href="{$_layoutParams.root}admin/cliente/editar/{$cliente.cli_key}"><i class="icon-pencil"></i> Modificar</a>                {/if}                {if $_acl->permiso('eliminar_cliente')}                        <a class="btn btn-danger" href="{$_layoutParams.root}admin/cliente/eliminar/{$cliente.cli_key}"><i class="icon-trash"></i> Eliminar</a>                {/if}				</td>            </tr>        {/foreach}
    </table><div class="col-md-12 pull-right" >		<div align="right" >		{$paginacion|default:""}	</div>	</div>	
</form>