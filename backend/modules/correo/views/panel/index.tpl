ogress">													<div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: {($fun.cantidad*100)/13}%" title="{($fun.cantidad*100)/13}%">														<span class="sr-only"></span>													</div>												</div>											</td>										</tr>									{/foreach}									</tbody>								</table>												</div>						</div>							</div>					</div>						<div class="panel panel-default">			<div class="panel-heading">				<div class="panel-title">					<h4>						Ultimos PQRD					</h4>				</div>								<div class="panel-options">				<a href="#" data-rel="collapse"><i class="entypo-down-open" style="margin-left:-220px;"></i></a>					<div align="" style="margin-top:-27px;margin-left:-90px;">								<a id="listado" href="{$_layoutParams.root}wf/panel/listapqrd/"  class="btn btn-primary" align="center"><span ><b> Ver listado PQRD's</b></span></a>							</div>					<div align="right" style="margin-top:-31px;margin-left:50px;">								<a href="{$_layoutParams.root}wf/panel/nuevo/"  class="btn btn-red" align="center"><span ><b> Nueva PQRD </b></span></a>							</div>									</div>			</div>					<div class="panel-body no-padding">			<div id="pqrd">				<table class="table table-bordered table-hover" align="center" style="width: 100%;">					<tr>						<th>ID</th>						<th>Contenido</th>						<th>Tipo</th>						<th> </th>											</tr>					{$con=0}					{foreach $listadoscr as $lista}					{if $con < 6}					<tr>						<td>							{$lista.scr_key}						</td>						<td width="60%">							{$lista.scr_contenido}						</td>						<td>							{$lista.tpe_nombre}						</td>						<td align="right">							<a href="{$_layoutParams.root}wf/panel/detalle/{$lista.scr_key}" class="btn btn-blue">Detalles</a>						</td>					</tr>					{$con=$con+1}					{/if}					{/foreach}				</table>			</div>			</div>		</div>			</div>	